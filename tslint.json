{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-airbnb"],
  "jsRules": {},
  "rules": {
    "no-console": {
      "severity": "warning"
    },
    "interface-name": false,
    "ordered-imports": false,
    "variable-name": false,
    "import-name": false,
    "object-shorthand-properties-first": false,
    "no-submodule-imports": false,
    "member-access": false,
    "no-shadowed-variable": false
  },
  "rulesDirectory": []
}